---
import CommingSoonProjectPreview from "../components/commingSoonProjectPreview";
import SingleProjectPreview from "../components/singleProjectPreview";
import Layout from "../layouts/Layout.astro";

---

<Layout title="NotNullDev.com" >
    <div class="flex flex-1  gap-6 ">
    <div class="mx-auto flex gap-8 justify-center flex-wrap">
        <SingleProjectPreview
        title="E-commerce"
        description="Simple e-commerce site built with Next.js, prisma, trpc, tailwind, nextauth, stripe and daisyui."
        imageUrl="https://cdn.notnulldev.com/e-com.webp"
        linkUrl="https://e-com.notnulldev.com/"
    />

    <SingleProjectPreview
        title="Local business"
        description="Sample website for local business"
        imageUrl="https://cdn.notnulldev.com/daxer.webp"
        linkUrl="https://daxer.notnulldev.com/"
        />

    <CommingSoonProjectPreview
    title="Nano CI/CD"
    description="Simple CI/CD built with Next.js, mui, tailwind and Go on the backend."
    imageUrl="https://cdn.notnulldev.com/nano-ui.webp"
    />

    <CommingSoonProjectPreview
    title="Nano management"
    description="Application to report time for teams build with Next.js, Mantine, tailwind and pocketbase on the backend."
    imageUrl="https://cdn.notnulldev.com/nano-management.webp"
/>

    </div>

    </div>
</Layout>
